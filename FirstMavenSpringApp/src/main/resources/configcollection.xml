<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns = "http://www.springframework.org/schema/beans"
   xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation = "http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">


	<bean id="stdObj"  class="com.java.spack.StudentInfo">
	
		<property name="sname"  value="Akhila"/>
		
		<property name="qualifiaction">
			<list>
				<value>B.Tech</value>
				<value>M.Tech</value>
			</list>		
		</property>
		
		<property name="courses">
			<set>
				<value>Java</value>
				<value>Dotnet</value>
			</set>
		</property>
	
		<property name="course_and_faculty">
			<map>
				<entry key="Java" value="Venugopal"/>
				<entry key="Dotnet" value="Priya Bhavani"/>
			</map>
		</property>
		
		<property name="course_and_fees">
			<props>
				<prop key="Java">15000.00</prop>
				<prop key="Dotnet">25000.00</prop>
			</props>
		</property>
		
	</bean>	
</beans>